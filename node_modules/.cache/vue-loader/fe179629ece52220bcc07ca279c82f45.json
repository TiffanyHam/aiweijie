            this.initPages()\r\n        },\r\n        binData(n, o) {\r\n        },\r\n    },\r\n\r\n    methods: {\r\n        /**\r\n         * @description: 初始化页面\r\n         * @param {*}\r\n         * @return {*}\r\n         */\r\n        initPages() {\r\n            this.mode = this.$route.params.type === 'U' ? '0' : '1';\r\n            this.text_cur = this.mode === \"0\" ? this.text_U : this.text_I;\r\n        },\r\n    },\r\n};\r\n</script>\r\n<style lang='less' scoped>\r\n.brushMethods {\r\n    background-color: #fff;\r\n    width: 100%;\r\n    height: 100%;\r\n    padding: 0 8px;\r\n    .mar {\r\n        height: 16px;\r\n    }\r\n    .methods_main {\r\n        flex: 1;\r\n        padding: 0 16px 0;\r\n        overflow: hidden;\r\n        .wrapper {\r\n            padding: 0 0 24px 0;\r\n            .methods_item {\r\n                margin-bottom: 24px;\r\n                .title {\r\n                    color: rgba(0, 0, 0, 0.9);\r\n                    font-size: 19px;\r\n                    margin-bottom: 16px;\r\n                }\r\n                .subtitle {\r\n                    color: rgba(0, 0, 0, 0.6);\r\n                    font-size: 13px;\r\n                    line-height: 18px;\r\n                }\r\n                .img {\r\n                    height: 215px;\r\n                    width: 312px;\r\n                    margin: 16px 0;\r\n                    .back_img {\r\n                        display: inline-block;\r\n                        background-size: 100% 100%;\r\n                        background-repeat: no-repeat;\r\n                        height: 100%;\r\n                        width: 100%;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\nbutton :focus {\r\n    outline: 0;\r\n}\r\n.flexC {\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n.flex {\r\n    display: flex;\r\n    flex-direction: row;\r\n}\r\n\r\n.theme-dark {\r\n    .brushMethods {\r\n        background-color: